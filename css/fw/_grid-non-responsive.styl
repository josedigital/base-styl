lesscss-percentage(n)
  (n * 100)%
.clear:before, .clear:after
  content " "
  display table
.clear:after
  clear both
.row:before, .row:after
  content ""
  display table
.row:after
  clear both
.row
  position relative
  margin-left -15px
  margin-right -15px
.container
  padding-left 15px
  padding-right 15px
  margin-left auto
  margin-right auto
  width desktop-container
.container-full
  padding-left 0
  padding-right 0
  margin-left auto
  margin-right auto
.col-1, .col-2, .col-3, .col-4, .col-5, .col-6, .col-7, .col-8, .col-9, .col-10, .col-11, .col-12
  padding-left 15px
  padding-right 15px
  position relative
  float left
generate-grid(12)
generate-grid(index)
  generate-grid((index - 1))
  .col-index
    width ((index / 12) * 100%)
generate-push-offsets(11)
generate-push-offsets(index)
  generate-push-offsets((index - 1))
  .push-index
    left ((index / 12) * 100%)
generate-pull-offsets(11)
generate-pull-offsets(index)
  generate-pull-offsets((index - 1))
  .pull-index
    left ((- index index / 12) * 100%)
